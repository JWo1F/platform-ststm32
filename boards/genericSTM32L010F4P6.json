{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m0",
    "extra_flags": "-DSTM32L010x4",
    "f_cpu": "32000000L",
    "ldscript": "STM32L010F4Px_FLASH.ld",
    "mcu": "stm32l010f4p6",
    "variant": "stm32l0"
  },
  "debug": {
    "default_tool": "stlink",
    "onboard_tools": ["stlink"],
    "openocd_target": "stm32l0",
    "svd_path": "STM32L0x0.svd"
  },
  "frameworks": ["mbed", "stm32cube"],
  "name": "Generic STM32L010F4P6",
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 16384,
    "protocol": "stlink",
    "protocols": ["jlink", "stlink", "mbed"],
    "require_upload_port": false
  },
  "url": "https://www.st.com/en/microcontrollers-microprocessors/stm32l010f4.html",
  "vendor": "ST"
}
